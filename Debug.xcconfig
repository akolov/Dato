#include "Common.xcconfig"

// Architectures

ONLY_ACTIVE_ARCH = YES

// Deployment

COPY_PHASE_STRIP = NO

// Apple LLVM - Code Generation

GCC_OPTIMIZATION_LEVEL = 0
GCC_UNROLL_LOOPS = NO
LLVM_LTO = NO

// Apple LLVM - Preprocessing

GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1 DEBUG=1
